{
  "name": "Rhymey",
  "short_name": "Rhymey",
  "description": "Get rhymes for double-clicked words, even in Google Docs!",
  "version": "3.0.2",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "http://localhost/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "custom.css"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}